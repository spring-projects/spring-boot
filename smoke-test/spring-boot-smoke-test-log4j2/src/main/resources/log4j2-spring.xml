<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
	<SpringProfile name="staging">
		<Loggers>
			<Logger name="smoketest.log4j2" level="TRACE"/>
			<Root level="INFO">
				<AppenderRef ref="Console"/>
			</Root>
		</Loggers>
	</SpringProfile>
	<Loggers>
		<Logger name="smoketest.log4j2" level="DEBUG"/>
		<Root level="INFO">
			<AppenderRef ref="Console"/>
		</Root>
	</Loggers>
</Configuration>
